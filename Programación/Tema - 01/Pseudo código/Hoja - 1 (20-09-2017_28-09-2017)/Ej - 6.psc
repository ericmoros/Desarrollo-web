Proceso H1_Ej6
	Definir num1, num2, num3, result Como Real;
	
	
	Escribir "Introduce un número";
	
	
	Si num1 <= 0 Entonces
		result <- num1 * num2 * num3;
			Escribir "El producto de los tres números es: ", result;
	Sino
		result <- num1 + num2 + num3;
			Escribir "La suma de los tres números es: ", result;	
	FinSi
